@article{avshalumov_activation_2003,
	title = {Activation of {ATP}-sensitive {K} $^{\textrm{+}}$ ({K} $_{\textrm{{ATP}}}$ ) channels by {H} $_{\textrm{2}}$ {O} $_{\textrm{2}}$ underlies glutamate-dependent inhibition of striatal dopamine release},
	volume = {100},
	issn = {0027-8424},
	doi = {10.1073/pnas.1834314100},
	abstract = {{\textless}p{\textgreater} In many cells, ATP-sensitive K $^{\textrm{+}}$ channels (K $_{\textrm{ATP}}$ channels) couple metabolic state to excitability. In pancreatic beta cells, for example, this coupling regulates insulin release. Although K $_{\textrm{ATP}}$ channels are abundantly expressed in the brain, their physiological role and the factors that regulate them are poorly understood. One potential regulator is H $_{\textrm{2}}$ O $_{\textrm{2}}$ . We reported previously that dopamine (DA) release in the striatum is modulated by endogenous H $_{\textrm{2}}$ O $_{\textrm{2}}$ , generated downstream from glutamatergic α-amino-3-hydroxy-5-methyl-4-isoxazolepropionic acid (AMPA)-receptor activation. Here we investigated whether H $_{\textrm{2}}$ O $_{\textrm{2}}$ -sensitive K $_{\textrm{ATP}}$ channels contribute to DA-release modulation by glutamate and γ-aminobutyric acid (GABA). This question is important because DA–glutamate interactions underlie brain functions, including motor control and cognition. Synaptic DA release was evoked by using local electrical stimulation in slices of guinea pig striatum and monitored in real time with carbon-fiber microelectrodes and fast-scan cyclic voltammetry. The K $_{\textrm{ATP}}$ -channel antagonist glibenclamide abolished the H $_{\textrm{2}}$ O $_{\textrm{2}}$ -dependent increase in DA release usually seen with AMPA-receptor blockade by GYKI-52466 [1-(4-aminophenyl)-4-methyl-7,8-methylenedioxy-5 {\textless}italic{\textgreater}H{\textless}/italic{\textgreater} -2,3-benzodiazepine hydrochloride] and the decrease in DA release seen with GABA-type-A-receptor blockade by picrotoxin. In contrast, 5-hydroxydecanoate, a mitochondrial K $_{\textrm{ATP}}$ -channel blocker, was ineffective, as were sulpiride, a D $_{\textrm{2}}$ -receptor antagonist, and tertiapin, a G protein-coupled K $^{\textrm{+}}$ -channel inhibitor. Diazoxide, a sulfonylurea receptor 1 (SUR1)selective K $_{\textrm{ATP}}$ -channel opener, prevented DA modulation by H $_{\textrm{2}}$ O $_{\textrm{2}}$ , glutamate, and GABA, whereas cromakalim, a SUR2-selective opener, did not. Thus, endogenous H $_{\textrm{2}}$ O $_{\textrm{2}}$ activates SUR1-containing K $_{\textrm{ATP}}$ channels in the plasma membrane to inhibit DA release. These data not only demonstrate that K $_{\textrm{ATP}}$ channels can modulate CNS transmitter release in response to fast-synaptic transmission but also introduce H $_{\textrm{2}}$ O $_{\textrm{2}}$ as a K $_{\textrm{ATP}}$ -channel regulator. {\textless}/p{\textgreater}},
	number = {20},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Avshalumov, Marat V. and Rice, Margaret E.},
	month = sep,
	year = {2003},
	pages = {11729--11734},
}

