	pages = {122},
	file = {Full Text PDF:/Users/paulhouston/Zotero/storage/RP2FG2XA/Rakshasa-Loots et al. - 2025 - Metabolic biomarkers of clinical outcomes in severe mental illness (METPSY) protocol for a prospect.pdf:application/pdf},
