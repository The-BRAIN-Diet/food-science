@article{chang_cortisol_2021-1,
	title = {Cortisol and inflammatory biomarker levels in youths with attention deficit hyperactivity disorder ({ADHD}): evidence from a systematic review with meta-analysis},
	volume = {11},
	issn = {2158-3188},
	doi = {10.1038/s41398-021-01550-0},
	abstract = {{\textless}p{\textgreater} Several studies reported abnormal cortisol and inflammatory biomarker levels in youths with attention deficit hyperactivity disorder (ADHD), but the results have not been conclusive. We conducted a systematic review followed by a meta-analysis of case-control studies assessing blood or saliva cortisol levels and blood levels of inflammatory biomarkers in youth with ADHD. The effect sizes (ES) were synthesized by using a random-effects model. In the 19 studies on cortisol levels (totaling {\textless}italic{\textgreater}n{\textless}/italic{\textgreater}  = 916 youth with ADHD and {\textless}italic{\textgreater}n{\textless}/italic{\textgreater}  = 947 typically developing (TD), healthy youth), youth with ADHD have lower basal cortisol levels at any time-points during the day (effect size: .68; {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.004) and lower cumulative levels of cortisol (ES: .39, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = .008) throughout the day than TD youth. Moreover, morning cortisol levels were lower in ADHD youth when compared with TD youth (14 studies, {\textless}italic{\textgreater}n{\textless}/italic{\textgreater}  = 1679, ES: .84, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.003), while there is no difference for the afternoon cortisol levels ( {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.48). The meta-analysis on inflammation biomarker was conducted on 4 studies (totaling {\textless}italic{\textgreater}n{\textless}/italic{\textgreater}  = 404 youth) showed that Tumour Necrosis Factor-alpha (TNF-α) was lower in ADHD when compared with TD (3 studies, {\textless}italic{\textgreater}n{\textless}/italic{\textgreater}  = 257 youth, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.004), while no differences for Interleukin-1β(IL-1β) ( {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.21), IL-6 ( {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.09) and IL-10 ( {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.77). The lower cortisol in the context of low TNF-α levels may indicate a specific pattern of biomarkers in ADHD, and further investigation is warranted. {\textless}/p{\textgreater}},
	number = {1},
	journal = {Translational Psychiatry},
	author = {Chang, Jane Pei-Chen and Su, Kuan-Pin and Mondelli, Valeria and Pariante, Carmine M.},
	month = aug,
	year = {2021},
	pages = {430},
}

