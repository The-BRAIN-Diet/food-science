	number = {4},
	urldate = {2025-09-10},
	journal = {Translational Psychiatry},
	author = {Volkow, N D and Wang, G-J and Logan, J and Alexoff, D and Fowler, J S and Thanos, P K and Wong, C and Casado, V and Ferre, S and Tomasi, D},
	month = apr,
	year = {2015},
	pages = {e549--e549},
	file = {Full Text:/Users/paulhouston/Zotero/storage/B3UN9L3S/Volkow et al. - 2015 - Caffeine increases striatal dopamine D2D3 receptor availability in the human brain.pdf:application/pdf},
}

