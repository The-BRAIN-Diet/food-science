@article{michael_randomised_2020,
	title = {A randomised controlled study shows supplementation of overweight and obese adults with lactobacilli and bifidobacteria reduces bodyweight and improves well-being},
	volume = {10},
	issn = {2045-2322},
	doi = {10.1038/s41598-020-60991-7},
	abstract = {{\textless}p{\textgreater} In an exploratory, block-randomised, parallel, double-blind, single-centre, placebo-controlled superiority study (ISRCTN12562026, funded by Cultech Ltd), 220 Bulgarian participants (30 to 65 years old) with BMI 25–34.9 kg/m $^{\textrm{2}}$ received Lab4P probiotic (50 billion/day) or a matched placebo for 6 months. Participants maintained their normal diet and lifestyle. Primary outcomes were changes in body weight, BMI, waist circumference (WC), waist-to-height ratio (WtHR), blood pressure and plasma lipids. Secondary outcomes were changes in plasma C-reactive protein (CRP), the diversity of the faecal microbiota, quality of life (QoL) assessments and the incidence of upper respiratory tract infection (URTI). Significant between group decreases in body weight (1.3 kg, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001), BMI (0.045 kg/m $^{\textrm{2}}$ , {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001), WC (0.94 cm, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001) and WtHR (0.006, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001) were in favour of the probiotic. Stratification identified greater body weight reductions in overweight subjects (1.88\%, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001) and in females (1.62\%, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.0005). Greatest weight losses were among probiotic hypercholesterolaemic participants (−2.5\%, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001) alongside a significant between group reduction in small dense LDL-cholesterol (0.2 mmol/L, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  = 0.0241). Improvements in QoL and the incidence rate ratio of URTI (0.60, {\textless}italic{\textgreater}p{\textless}/italic{\textgreater}  \&lt; 0.0001) were recorded for the probiotic group. No adverse events were recorded. Six months supplementation with Lab4P probiotic resulted in significant weight reduction and improved small dense low-density lipoprotein-cholesterol (sdLDL-C) profiles, QoL and URTI incidence outcomes in overweight/obese individuals. {\textless}/p{\textgreater}},
	number = {1},
	journal = {Scientific Reports},
	author = {Michael, D. R. and Jack, A. A. and Masetti, G. and Davies, T. S. and Loxley, K. E. and Kerry-Smith, J. and Plummer, J. F. and Marchesi, J. R. and Mullish, B. H. and McDonald, J. A. K. and Hughes, T. R. and Wang, D. and Garaiova, I. and Paduchová, Z. and Muchová, J. and Good, M. A. and Plummer, S. F.},
	month = mar,
