	keywords = {Female, Humans, Male, Adult, Middle Aged, Aged, Dose-Response Relationship, Drug, Folic Acid, Food, Formulated, Hematinics, Homocysteine, Multivariate Analysis, Pyridoxine, Randomized Controlled Trials as Topic, Vitamin B 12},
	pages = {894--898},
}

