	urldate = {2025-09-11},
	journal = {The Journal of Neuroscience},
	author = {Huang, Jian and Zhong, Zhaomin and Wang, Mingyong and Chen, Xifeng and Tan, Yicheng and Zhang, Shuqing and He, Wei and He, Xiong and Huang, Guodong and Lu, Haiping and Wu, Ping and Che, Yi and Yan, Yi-Lin and Postlethwait, John H. and Chen, Wenbiao and Wang, Han},
	month = feb,
